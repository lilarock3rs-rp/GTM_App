{
  "version": 2,
  "public": true,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/api/test", "dest": "/api/simplified.js" },
    { "src": "/api", "dest": "/api/index.js" },
    { "src": "/api/hubspot", "dest": "/api/hubspot-test.js" },
    { "src": "/api/webhook", "dest": "/api/webhook.js" },
    
    { "handle": "filesystem" }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}